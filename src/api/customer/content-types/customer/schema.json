{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "customerId": {
      "type": "uid"
    },
    "address": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "username": {
      "type": "string"
    },
    "mail": {
      "type": "email"
    },
    "birthDay": {
      "type": "date"
    },
    "avatar": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "tasks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::task.task",
      "mappedBy": "customer"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "customer"
    }
  }
}
